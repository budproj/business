{
  "name": "business",
  "version": "1.0.0",
  "description": "Web interface for Bud's execution mode",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "sh ./scripts/postinstall.sh",
    "start": "node dist/src/main",
    "prebuild": "run-s clean",
    "build": "nest build",
    "format": "prettier --write 'src/**/*.ts'",
    "start:dev": "run-s start:watch",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "compile": "tsc",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf -rf dist",
    "lint": "eslint '{src,lib}/**/*.ts'",
    "lint:staged": "lint-staged",
    "lint:fix": "npm run lint -- --fix",
    "typeorm": "run-s clean compile && npm run typeorm:run --",
    "typeorm:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --config src/config/database/config",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:format": "prettier --write 'src/migrations/*.ts'",
    "db:seed": "./scripts/seed.sh",
    "db:production:load-data": "ts-node -r tsconfig-paths/register src/database/environments/production/load-dump",
    "scripts:create-and-invite-users": "run-s build scripts:create-users scripts:invite-users",
    "scripts:create-users": "node ./scripts/createUsers.js",
    "scripts:invite-users": "node ./scripts/inviteUsers.js",
    "deploy": "run-s git:tag:canary build waypoint:build waypoint:deploy",
    "release": "run-s git:tag:remove-canary git:tag:stable waypoint:up",
    "git:tag:canary": "git tag canary -f && git push origin canary -f",
    "git:tag:remove-canary": "git tag -d canary",
    "git:tag:stable": "git tag stable -f && git push origin stable -f",
    "waypoint:build": "waypoint build",
    "waypoint:deploy": "waypoint deploy",
    "waypoint:up": "waypoint up"
  },
  "author": "delucca <delucca@pm.me>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/budproj/business.git"
  },
  "bugs": {
    "url": "https://github.com/budproj/business/issues"
  },
  "engines": {
    "npm": ">=7.3.0",
    "node": ">=15.5.1"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/common": "^7.5.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.5.2",
    "@nestjs/graphql": "^7.8.2",
    "@nestjs/passport": "^7.1.3",
    "@nestjs/platform-fastify": "^7.5.2",
    "@nestjs/typeorm": "^7.1.5",
    "@types/lodash": "^4.14.165",
    "@types/passport-jwt": "^3.0.3",
    "apollo-server-core": "^2.16.1",
    "apollo-server-fastify": "^3.0.0-alpha.3",
    "auth0": "^2.31.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "date-fns": "^2.16.1",
    "deepmerge": "^4.2.2",
    "fast-csv": "^4.3.6",
    "fastify": "^3.8.0",
    "fastify-cors": "^5.1.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.1",
    "jwks-rsa": "^1.11.0",
    "localtunnel": "^2.0.1",
    "lodash": "^4.17.20",
    "logform": "^2.2.0",
    "nest-winston": "^1.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "secure-random-password": "^0.2.2",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@budproj/eslint-config-base-ts": "^1.0.3",
    "@budproj/eslint-config-prettier-ts": "^1.0.0",
    "@budproj/prettier-config": "^1.0.0",
    "@nestjs/schematics": "^7.2.1",
    "@nestjs/testing": "^7.6.11",
    "@types/auth0": "^2.20.12",
    "@types/eslint": "^7",
    "@types/eslint-config-prettier": "^6",
    "@types/eslint-plugin-prettier": "^3",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.22",
    "@types/localtunnel": "^2.0.0",
    "@types/passport": "^0",
    "@types/prettier": "^2",
    "@types/rimraf": "^3",
    "@types/secure-random-password": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "faker": "^5.3.1",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "node-sql-reader": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
