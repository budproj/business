{
  "name": "business",
  "version": "0.0.1",
  "description": "Web interface for Bud's execution mode",
  "main": "src/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write 'src/**/*.ts'",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,lib}/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --config src/config/database/config",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run"
  },
  "author": "delucca <delucca@pm.me>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/budproj/business.git"
  },
  "bugs": {
    "url": "https://github.com/budproj/business/issues"
  },
  "engines": {
    "npm": ">=7.0.8",
    "node": ">=15.2.1"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.5.2",
    "@nestjs/passport": "^7.1.3",
    "@nestjs/platform-fastify": "^7.5.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "fastify": "^3.8.0",
    "jwks-rsa": "^1.11.0",
    "logform": "^2.2.0",
    "nest-winston": "^1.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@budproj/eslint-config-base-ts": "^0.0.12",
    "@budproj/eslint-config-prettier-ts": "^0.0.12",
    "@budproj/prettier-config": "^0.0.12",
    "@nestjs/cli": "^7.5.2",
    "@nestjs/schematics": "^7.2.1",
    "@types/eslint": "^7",
    "@types/eslint-config-prettier": "^6",
    "@types/eslint-plugin-prettier": "^3",
    "@types/passport": "^0",
    "@types/prettier": "^2",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint"
    }
  }
}
